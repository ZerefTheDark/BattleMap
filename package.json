{
  "name": "battlemap-frontend",
  "version": "1.0.0",
  "description": "BattleMap D&D Battle Map Application with Playwright support",
  "main": "index.html",
  "scripts": {
    "start": "python3 -m http.server 8000",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "install:playwright": "playwright install",
    "install:deps": "playwright install-deps",
    "mcp:start": "node mcp-server.js",
    "dev": "python3 -m http.server 8000"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0"
  },
  "keywords": [
    "dnd",
    "battlemap",
    "tabletop",
    "rpg",
    "playwright",
    "mcp"
  ],
  "author": "Chris Marshall",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZerefTheDark/BattleMap.git"
  }
}